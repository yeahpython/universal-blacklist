{
  "name": "Literal Censorship",
  "version": "0.0",
  "permissions": [
    "storage",
    "https://*/*",
    "http://*/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://*/*", "http://*/*"],
      "css": ["mystyles.css"],
      "js": ["third-party/jquery-1.12.0.min.js", "myscript.js"],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "128": "images/icon128.png"
    },
  "browser_action": {
    "default_icon": {                    // optional
      "19": "images/icon19.png",           // optional
      "38": "images/icon38.png"            // optional
    },
    "default_title": "Censorship Settings",      // optional; shown in tooltip
    "default_popup": "popup.html"        // optional
  },
  "manifest_version": 2
}